<html t:type="layout" title="Structural JavaScript Patterns"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

	<img id="dancingmoosenbeaver" src="${context:img/moosenbeaverdancing.gif}" style="position: absolute; left: 250px; top: 200px;" />
	<img id="green-ball" src="${context:img/green_ball.gif}" style="position: absolute; left: 250px; top: 200px;" />

	<img id="moosenbeaver" src="${context:img/moosenbeaver.gif}" style="position: absolute; left: 500px; top: 100px;" />

    <div style="width: 200px; height: 400px; background-color: black; float: left;">
        <div style="color: white; padding: 120px 0 0 95px;"><b>E<br />N<br />D<br />Z<br />O<br />N<br />E</b></div>
    </div>

    <div style="width: 200px; height: 400px; background-color: black; float: right;">
        <div style="color: white; padding: 120px 0 0 95px;"><b>E<br />N<br />D<br />Z<br />O<br />N<br />E</b></div>
    </div>

    <br clear="both" />

    <div style="margin-left: auto; margin-right: auto; width: 600px;">
        <div style="float: left; border: solid black 2px; padding: 10px; margin: 10px; width: 250px;">
            <h1>Dancing Beaver</h1>
            <b>Score: 0</b>
        </div>
        <div style="float: left; border: solid black 2px; padding: 10px; margin: 10px; width: 250px;">
            <h1>Albino Beaver</h1>
            <b>Score: 0</b>
        </div>
    </div>

	<!-- Uncomment this section to try module-multiple-instances.js -->

