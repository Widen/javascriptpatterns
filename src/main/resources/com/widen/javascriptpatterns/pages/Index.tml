<html t:type="layout" title="Structural JavaScript Patterns"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

	<img id="dancingmoosenbeaver" src="${context:img/moosenbeaverdancing.gif}" class="dancing" />
	<img id="green-ball" src="${context:img/green_ball.gif}" class="ball" />

	<img id="moosenbeaver" src="${context:img/moosenbeaver.gif}" class="albino" />

    <div class="endzone fLeft">
        <div class="endzone-text"><b>E<br />N<br />D<br />Z<br />O<br />N<br />E</b></div>
    </div>

    <div class="endzone fRight">
        <div class="endzone-text"><b>E<br />N<br />D<br />Z<br />O<br />N<br />E</b></div>
    </div>

    <br clear="both" />

    <div class="scores">
        <div class="score">
            <h1>Dancing Beaver</h1>
            <b>Score: 0</b>
        </div>
        <div class="score">
            <h1>Albino Beaver</h1>
            <b>Score: 0</b>
        </div>
    </div>

